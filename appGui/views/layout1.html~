    <!-- flot charts -->
    <script src="{{=URL('static','js/jquery.flot.js')}}"></script>
    <script src="{{=URL('static','js/jquery.flot.stack.js')}}"></script>
    <script src="{{=URL('static','js/jquery.flot.resize.js')}}"></script>
